{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/ngx-slider-recaptcha",
  "deleteDestPath": true,
  "lib": {
    "entryFile": "src/public-api.ts",
    "styleIncludePaths": [
      "src"
    ]
  },
  "assets": [
    {
      "glob": "**/*",
      "input": "src/assets/images",
      "output": "assets/images"
    }
  ]
}