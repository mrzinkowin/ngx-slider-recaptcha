{
  "name": "ngx-slider-recaptcha",
  "version": "1.1.0-beta",
  "description": "Angular slider with reCAPTCHA verification",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:demo": "ng serve slider-recaptcha-demo",
    "start:custom-demo": "ng serve slider-recaptcha-custom-demo",
    "build": "ng build",
    "build:lib": "ng build ngx-slider-recaptcha",
    "build:custom-demo": "ng build slider-recaptcha-custom-demo --base-href https://mrzinkowin.github.io/ngx-slider-recaptcha/",
    "deploy:custom-demo": "npm run build:custom-demo && angular-cli-ghpages --dir=dist/slider-recaptcha-custom-demo",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "publish:lib": "npm run build:lib && cd dist/ngx-slider-recaptcha && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrzinkowin/ngx-slider-recaptcha"
  },
  "author": {
    "name": "Zin Ko Win",
    "email": "zinkowin.developer@gmail.com"
  },
  "keywords": [
    "angular",
    "angular-library",
    "slider",
    "captcha",
    "recaptcha",
    "slider-captcha",
    "slider-recaptcha",
    "captcha-library",
    "reCAPTCHA-verification",
    "drag-and-drop-captcha",
    "slider-validation",
    "ng-recaptcha",
    "angular-captcha",
    "angular-slider-captcha",
    "angular-slider-library"
  ],
  "bugs": {
    "url": "https://github.com/mrzinkowin/ngx-slider-recaptcha/issues"
  },
  "peerDependencies": {
    "@angular/common": "^14.0.0",
    "@angular/core": "^14.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.3",
    "@angular/animations": "^14.1.0",
    "@angular/cli": "~14.1.2",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@types/jasmine": "~4.0.0",
    "angular-cli-ghpages": "^2.0.3",
    "bootstrap": "^5.3.3",
    "font-awesome": "^4.7.0",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.1.0",
    "ng-zorro-antd": "^14.3.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "typescript": "~4.7.2",
    "zone.js": "~0.11.4"
  }
}