{
  "name": "ngx-slider-recaptcha",
  "version": "1.0.0",
  "description": "Angular slider with reCAPTCHA verification",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prebuild:lib": "cpx README.md projects/ngx-slider-recaptcha/ && cpx CHANGELOG.md projects/ngx-slider-recaptcha/ && cpx LICENSE projects/ngx-slider-recaptcha/",
    "build:lib": "npm run prebuild:lib && ng build ngx-slider-recaptcha && npm run postbuild:lib",
    "postbuild:lib": "rimraf projects/ngx-slider-recaptcha/README.md projects/ngx-slider-recaptcha/CHANGELOG.md projects/ngx-slider-recaptcha/LICENSE",
    "publish:lib": "npm run build:lib && cd dist/ngx-slider-recaptcha && npm publish --access public"
  },
  "private": true,
  "dependencies": {
    "font-awesome": "^4.7.0",
    "tslib": "^2.3.0"
  },
  "peerDependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "font-awesome": "^4.7.0",
    "rxjs": "~7.5.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.3",
    "@angular/cli": "~14.1.2",
    "@angular/compiler-cli": "^14.1.0",
    "@types/jasmine": "~4.0.0",
    "cpx": "^1.5.0",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.1.0",
    "typescript": "~4.7.2"
  }
}